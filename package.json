{
  "name": "cnegru-twilio-serverless",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:server": "tsc -w & twilio-run --no-assets",
    "start:client": "cd ./app && npm start",
    "build:client": "cd ./app && npm run build",
    "build:server": "tsc",
    "deploy": "npm run build:client && twilio-run deploy --override-existing-project"
  },
  "dependencies": {
    "@twilio-labs/serverless-runtime-types": "^2.1.1",
    "@twilio/runtime-handler": "1.1.3",
    "twilio": "^3.56"
  },
  "devDependencies": {
    "twilio-run": "^3.2.2",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": "12"
  }
}
